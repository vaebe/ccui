{
  "name": "vue3-ccui",
  "version": "1.2.2",
  "license": "MIT",
  "description": "vue3-ccui components based on Vite and Vue3",
  "keywords": [
    "frontend",
    "typescript",
    "ui-component",
    "components",
    "vue-components",
    "vue",
    "vue3",
    "vite",
    "jsx",
    "vue3-ccui"
  ],
  "homepage": "https://vaebe.github.io/ccui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/vaebe/ccui.git"
  },
  "main": "vue-ccui.umd.js",
  "module": "vue-ccui.es.js",
  "style": "style.css",
  "scripts": {
    "dev": "vitepress dev docs",
    "predev": "node ../cli/index.js create -t ccui --ignore-parse-error && node ../cli/index.js generate:theme",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vitepress-theme-demoblock": "^2.0.2",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.3.2",
    "c8": "^7.13.0",
    "jsdom": "^22.0.0",
    "moment": "^2.29.4",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-svg-loader": "^3.6.0",
    "vitepress": "1.0.0-alpha.75",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.5"
  }
}
