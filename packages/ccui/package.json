{
  "name": "vue3-ccui",
  "version": "1.2.2",
  "license": "MIT",
  "description": "vue3-ccui components based on Vite and Vue3",
  "keywords": [
    "frontend",
    "typescript",
    "ui-component",
    "components",
    "vue-components",
    "vue",
    "vue3",
    "vite",
    "jsx",
    "vue3-ccui"
  ],
  "homepage": "https://vaebe.github.io/ccui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/vaebe/ccui.git"
  },
  "main": "vue-ccui.umd.js",
  "module": "vue-ccui.es.js",
  "style": "style.css",
  "scripts": {
    "dev": "vitepress dev docs",
    "predev": "node ../cli/index.js create -t ccui --ignore-parse-error && node ../cli/index.js generate:theme",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vitepress-theme-demoblock": "^2.0.2",
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@types/node": "^20.14.15",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "c8": "^10.1.2",
    "jsdom": "^24.1.1",
    "moment": "^2.30.1",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "^1.3.2",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  }
}
